# Address of the deployed Bank contract
VITE_BANK_ADDRESS=0x0000000000000000000000000000000000000000

# Address of the deployed PermitToken contract used for deposits and purchases
VITE_TOKEN_ADDRESS=0x0000000000000000000000000000000000000000

# Address of the PermitNFT collection
VITE_NFT_ADDRESS=0x0000000000000000000000000000000000000000

# Address of the NFTMarket contract
VITE_MARKET_ADDRESS=0x0000000000000000000000000000000000000000

# Public signer that issues whitelist signatures for permitBuy (optional)
VITE_WHITELIST_SIGNER=0x0000000000000000000000000000000000000000

# Target chain id (defaults to 11155111 if not set)
VITE_CHAIN_ID=11155111

# Chain name used in the UI (optional)
VITE_CHAIN_NAME=Sepolia Testnet

# RPC endpoint for the selected chain
VITE_RPC_URL=https://1rpc.io/sepolia

# WalletConnect project ID for AppKit (get one at https://cloud.reown.com)
VITE_WALLETCONNECT_PROJECT_ID=your_project_id_here
