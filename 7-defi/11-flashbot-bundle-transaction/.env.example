# Core RPC + accounts
SEPOLIA_RPC_URL="https://sepolia.infura.io/v3/<project-id>"
OWNER_PRIVATE_KEY="0xowner"
BUYER_PRIVATE_KEY="0xbuyer"
FLASHBOTS_SIGNER_KEY="0xflashbots"

# Contract configuration
CONTRACT_ADDRESS="0xcontract"
PRESALE_AMOUNT="1"

# Bundle scheduling (how many future blocks to queue per bundle)
BUNDLE_BLOCKS_AHEAD="5"

# Gas tuning (optional overrides; defaults are 25/2 gwei)
MAX_FEE_PER_GAS_GWEI="25"
MAX_PRIORITY_FEE_PER_GAS_GWEI="2"

# Flashbots endpoints (optional overrides)
FLASHBOTS_RELAY_URL="https://relay-sepolia.flashbots.net"
FLASHBOTS_STATUS_URL="https://protect-sepolia.flashbots.net/tx/"

# If you use foundry account to deploy the demo contract (optional)
DEPLOYER_ACCOUNT=
