# Copy this to .env and fill in your actual values
PRIVATE_KEY=your_wallet_private_key_here
MINT_TO=your_wallet_address_here

# Upload your metadata files to IPFS and use those URLs:
TOKEN_URI_0=ipfs://your_metadata_hash_for_token_1
TOKEN_URI_1=ipfs://your_metadata_hash_for_token_2  
TOKEN_URI_2=ipfs://your_metadata_hash_for_token_3

# After deployment, set this to your deployed contract address:
CONTRACT_ADDRESS=0x1234567890123456789012345678901234567890

# Polygon Mainnet RPC (free, fast, OpenSea supported!)
POLYGON_RPC_URL=https://polygon-rpc.com

